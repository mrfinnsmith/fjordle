{
  "name": "fjordle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "build:analyze": "ANALYZE=true next build",
    "lighthouse:local": "npm run build && npm run start & sleep 5 && npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless' && pkill -f 'next start'",
    "perf:test": "npm run build && npm run start",
    "test:performance": "node scripts/test-performance.js",
    "perf:report": "node scripts/performance-report.js",
    "perf:baseline": "node scripts/performance-report.js",
    "quick:perf": "./scripts/quick-perf-test.sh",
    "dev:test": "npm run dev"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "next": "14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^9.0.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.15",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  }
}
